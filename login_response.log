var iframeList = ['index']
var currExpand = null
var activeAid = null
//点击上侧菜单定位到左侧菜单
$(document).on('click', '.iframe-tab-item', function() {
	var aid = $(this).data('aid')
	console.log(aid,'点iframe-tab-item')
	$('.iframe-tab-item').css('background', '#FFF');
	$('.iframe-tab-' + aid).css('background', '#e2f1ff');
	$('.iframe-inner').css('display', 'none')
	$('.iframe-inner-' + aid).css('display', 'block')
	activeAid = aid;
	let aidList = aid.split('-'),childList = [],itemList=[];
	if(aidList && aidList.length){
		let sum = '',it = '';
		aidList.forEach(function(item,index){
			if(index<aidList.length-1){
				if(index>0){
					sum = sum + '-' + item;
					childList.push(sum);

					it = it + '-' + item;
					itemList.push(it);


				}else{
					sum = 'children-'+item;
					childList.push(sum);

					it = 'item-'+item;
					itemList.push(it);
				}
			}
		})
	}
	// console.log(childList,'childList==');
	$('.left-aside-item-children').css('display', 'none')
	$('.left-aside-item-children-children').css('display', 'none')
	$('.left-aside-item-children').removeClass('choseBk')
	$('.left-aside-item-children-children').removeClass('choseBk')

	//箭头复原
	$('#left-aside>div').each(function(index,item){
		$(item).find('i').removeClass('arrow-expand').addClass('arrow');
	})
	//展开箭头
	if(itemList && itemList.length){
		itemList.forEach(function(item,index){
			$('#left-aside').find('.'+item).find('i').removeClass('arrow').addClass('arrow-expand')
			if(index == itemList.length - 1 ){
				currExpand = $('#left-aside').find('.'+item).data('aid')
			}
	    })
	}
	//显示二级三级菜单
	if(childList && childList.length){
		childList.forEach(function(item,index){
			$('#left-aside').find('.'+item).css('display', 'block');
		})
	}
	//加颜色
	$('#left-aside').find('[data-aid='+aid+']').addClass('choseBk')

	console.log(currExpand,'点上面')

})

