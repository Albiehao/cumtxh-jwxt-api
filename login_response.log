});
//关闭全部 关闭左侧 关闭右侧
$('.iframe-tabs').on('click', '.iframe-tab-item .close-box>div', function(){
	let that = $(this);
	if(that.hasClass('dis')){
		return
	}
	// iframeList
	var aid = $(this).parents('.iframe-tab-item').data('aid');
    var ids = []
	$(this).parents('.iframe-tabs').find('.iframe-tab-item').each(function(index,item){
		ids.push($(item).data('aid'));
	})
	let num = ids.indexOf(aid);
	iframeList=[];
	if(that.hasClass('close-all')){
		//只展示首页
		if(ids && ids.length){
			ids.forEach(function(item,index){
				console.log(that.parents('.iframe-container'))
				if(item!=='index'){
					that.parents('.iframe-container').find('.iframe-inner-' + item).remove()
					that.parents('.iframe-tabs').find('.iframe-tab-' + item).remove();
				}else{
					iframeList.push(item);
					that.parents('.iframe-container').find('.iframe-inner-' + item).css('display', 'block')
				}
			})
		}
		$('.left-aside-item-children').css('display', 'none')
		$('.left-aside-item-children-children').css('display', 'none')
		$('#left-aside>div').removeClass('choseBk');

		//箭头复原
		$('#left-aside>div').each(function(index,item){
			$(item).find('i').removeClass('arrow-expand').addClass('arrow');
		})
		currExpand = null;
		console.log('关闭全部');
	}
	//关闭左边
	if(that.hasClass('close-left')){
		if(ids && ids.length){
			ids.forEach(function(item,index){
				if(index<num && item!='index'){
					that.parents('.iframe-container').find('.iframe-inner-' + item).remove()
					that.parents('.iframe-tabs').find('.iframe-tab-' + item).remove();
				}else{
					iframeList.push(item);
				}
			})
	    }
		console.log('关闭左边');
	}
	//关闭右边
	if(that.hasClass('close-right')){
		if(ids && ids.length){
			ids.forEach(function(item,index){
				if(index>num && item!='index'){
					that.parents('.iframe-container').find('.iframe-inner-' + item).remove()
					that.parents('.iframe-tabs').find('.iframe-tab-' + item).remove();
				}else{
					iframeList.push(item);
				}
			})
    	}
		console.log('关闭右边');
	}
	that.parent().css('display','none');
	event.stopPropagation()
})

$(document).on('hover', '.iframe-tab-item', function() {
	var aid = $(this).data('aid')
	$('.close-tab-' + aid).css('display', 'inline');
	//$('.tab-text-' + aid).css('display', 'none');
	if (this.style.background !== 'rgb(226, 241, 255)') {
	    $(this).css('background', '#DDD')
