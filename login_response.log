		$('.close-tab-' + aid).css('color', '#333')
	}
})

$(document).on('mouseleave', '.iframe-tab-item', function() {
	var aid = $(this).data('aid')
	console.log('.close-tab-' + aid)
	$('.close-tab-' + aid).css('display', 'none')
	$('.close-tab-' + aid).css('color', '#0483d4')
	$(this).css('color', '#0483d4');
	//$('.tab-text-' + aid).css('display', 'inline-block');
	if (this.style.background !== 'rgb(226, 241, 255)') {
		$(this).css('background', '#FFF')
	};
	$(this).parent('.iframe-tabs').find('.iframe-tab-item').each(function(index,item){
     $(item).find('.close-box').css('display','none')
    })
})

$(document).on('click', '.close-tab', function(event) {
	var aid = $(this).data('aid')
	console.log(aid,'点.close-tab')
	$(this).remove();
	$('.iframe-tab-' + aid).remove();
	$('.iframe-inner-' + aid).remove();
	var iframeListTmp = [];
	for (var i = 0; i < iframeList.length; i++) {
		if (iframeList[i] !== aid) {
			iframeListTmp.push(iframeList[i])
		} else if (activeAid === aid) {
			activeAid = iframeListTmp[iframeListTmp.length - 1]
			$('.iframe-inner-' + activeAid).css('display', 'block');
			$('.iframe-tab-' + activeAid).css('background', '#e2f1ff');
		}
	}
	iframeList = iframeListTmp
	event.stopPropagation()
})

//点左侧二级菜单
$(document).on('click', '.left-aside-item-children', function() {
	var aid = $(this).data('aid')
	console.log('点.left-aside-item-children')
	if(!$(this).find('i').length){
      $('#left-aside>div').removeClass('choseBk');
	  $(this).addClass('choseBk');
	}
	$('.left-aside-item-children-children').css('display', 'none')
	if (currExpand + '' !== aid + '') {
		$('.children-' + aid).css('display', 'flex')
	}
	
	$('.arrow').remove();
	$('.arrow-expand').remove();
	for (var i = 0; i < menuRes.length; i++) {
		if (menuRes[i].propertyIsEnumerable('children') && menuRes[i].children.length > 0) {
			if (aid === i && aid !== currExpand) {
				$('.item-' + i).append('<i class="arrow-expand arrow-' + i + '"></i>')
			} else {
				$('.item-' + i).append('<i class="arrow arrow-' + i + '"></i>')
			}
		}
		if (menuRes[i].propertyIsEnumerable('children')) {
			for (var j = 0; j < menuRes[i].children.length; j++) {
				if (menuRes[i].children[j].propertyIsEnumerable('children') && menuRes[i].children[j].children.length > 0) {
					if (aid === (i + '-' + j) && currExpand !== aid) {
						$('.item-' + i + '-' + j).append('<i class="arrow-expand arrow-' + i + '-' + j + '"></i>')
						$('.arrow-' + i).remove();
						$('.item-' + i).append('<i class="arrow-expand arrow-' + i + '"></i>')
					} else if (currExpand === aid) {
						$('.item-' + i + '-' + j).append('<i class="arrow arrow-' + i + '-' + j + '"></i>')
						$('.arrow-' + i).remove();
						$('.item-' + i).append('<i class="arrow-expand arrow-' + i + '"></i>')
					} else {
						$('.item-' + i + '-' + j).append('<i class="arrow arrow-' + i + '-' + j + '"></i>')
					}
				} else {
					if (aid === (i + '-' + j) && menuRes[i].children[j].url) {
						$('.arrow-' + i).remove();
						$('.item-' + i).append('<i class="arrow-expand arrow-' + i + '"></i>')
					}
				}
				if (aid === (i + '-' + j)) {
					if (menuRes[i].children[j].url) {
						$('.iframe-inner').css('display', 'none')
						$('.iframe-tab-item').css('background', '#FFF');
						if (iframeList.indexOf(aid) === -1) {
							//$('.iframe-container').append('<iframe class="iframe-inner iframe-inner-' + aid + '" src="http://127.0.0.1/jwglxt/' + menuRes[i].children[j].url + '" />')
							$('.iframe-container').append('<iframe class="iframe-inner iframe-inner-' + aid + '" src="' + window.location.href.split('jwglxt')[0] + 'jwglxt' + menuRes[i].children[j].url + '" />')
							iframeList.push(aid)
							activeAid = aid
							$('.iframe-tabs').append('<div class="iframe-tab-item iframe-tab-' + aid + '" data-aid="' + aid + '"><span class="tab-text tab-text-' + aid + '">' + menuRes[i].children[j].name + '</span><span class="close-tab close-tab-' + aid + '" data-aid="' + aid + '">x</span></div>');
						} else {
							$('.iframe-inner-' + aid).css('display', 'block')
						}
						$('.iframe-tab-' + aid).css('background', '#e2f1ff');
					}
				}
			}
		}
	}
	
	if (currExpand !== aid) {
		currExpand = aid
	} else {
		currExpand = aid.split('-')[0]
	}
	console.log(currExpand,'二级')
})

